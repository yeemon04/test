<%@ Page Language="VB" MasterPageFile="~/MasterPage.master" AutoEventWireup="false" CodeFile="HowtodoAppointment.aspx.vb" Inherits="HowtodoAppointment" title="DatDoc Group/How to do Appointment" %>
<asp:Content ID="Content1" ContentPlaceHolderID="ContentPlaceHolder1" Runat="Server">
    <div style="z-index: 101; left: 70px; width: 704px; position: absolute; top: 31px;
        height: 220px">
        <p class="MsoNormal" style="text-justify: inter-ideograph; margin: 0in 0in 10pt;
            line-height: 150%; text-align: justify">
            <b style="mso-bidi-font-weight: normal"><span style="font-size: 13pt; line-height: 150%;
                font-family: 'Times New Roman','serif'; mso-fareast-font-family: 'Times New Roman'">
                How to do appointment<?xml namespace="" ns="urn:schemas-microsoft-com:office:office"
                    prefix="o" ?><o:p></o:p></span></b></p>
        <p class="MsoNormal" style="text-justify: inter-ideograph; margin: 0in 0in 10pt;
            line-height: 150%; text-align: justify">
            <span style="font-size: 12pt; line-height: 150%; font-family: 'Times New Roman','serif';
                mso-fareast-font-family: 'Times New Roman'">We accept the booking appointment of
                our patients via a web site or a telephone call to a receptionist at DatDoc.
                <o:p></o:p>
            </span>
        </p>
        <p class="MsoNormal" style="text-justify: inter-ideograph; margin: 0in 0in 10pt;
            line-height: 150%; text-align: justify">
            <span style="font-size: 12pt; line-height: 150%; font-family: 'Times New Roman','serif';
                mso-fareast-font-family: 'Times New Roman'">There is only one request to our patients.
                This is "<b style="mso-bidi-font-weight: normal"><i style="mso-bidi-font-style: normal">Please
                    book appointment for up to SIX weeks ahead of the current date</i></b>".<o:p></o:p></span></p>
        <p class="MsoNormal" style="text-justify: inter-ideograph; margin: 0in 0in 10pt;
            line-height: 150%; text-align: justify">
            <span style="font-size: 12pt; line-height: 150%; font-family: 'Times New Roman','serif';
                mso-fareast-font-family: 'Times New Roman'">To do appointment,<span style="mso-spacerun: yes">&nbsp;
                </span>click "<asp:HyperLink ID="hplContactUs" runat="server" ForeColor="Black"
                            NavigateUrl="~/HomePage.aspx" Width="36px" Font-Size="Large">Login</asp:HyperLink>
                "</span></p>
        <p class="MsoNormal" style="text-justify: inter-ideograph; margin: 0in 0in 10pt;
            line-height: 150%; text-align: justify">
            <span style="font-size: 12pt; line-height: 150%; font-family: 'Times New Roman','serif';
                mso-fareast-font-family: 'Times New Roman'">&nbsp;</span>&nbsp;</p>
        <p class="MsoNormal" style="text-justify: inter-ideograph; margin: 0in 0in 10pt;
            line-height: 150%; text-align: justify">
            <b style="mso-bidi-font-weight: normal"><span style="font-size: 13pt; line-height: 150%;
                font-family: 'Times New Roman','serif'; mso-fareast-font-family: 'Times New Roman'">
                Register Arrival<o:p></o:p></span></b></p>
        <p class="MsoNormal" style="text-justify: inter-ideograph; margin: 0in 0in 10pt;
            line-height: 150%; text-align: justify">
            <span style="font-size: 12pt; line-height: 150%; font-family: 'Times New Roman','serif';
                mso-fareast-font-family: 'Times New Roman'">When patients arrive at DatDoc, they
                can request to use a workstation in the reception area in order to register their
                arrival. Patients can search their appointment list by enter their <b style="mso-bidi-font-weight: normal">
                    <i style="mso-bidi-font-style: normal">Surname, Forename and month of their birth</i></b>.
                If the data is not found, please ask to the receptionist.
                <o:p></o:p>
            </span>
        </p>
        <p class="MsoNormal" style="text-justify: inter-ideograph; margin: 0in 0in 10pt;
            line-height: 150%; text-align: justify">
            <span style="font-size: 12pt; line-height: 150%; font-family: 'Times New Roman','serif';
                mso-fareast-font-family: 'Times New Roman'">Patients can also <b style="mso-bidi-font-weight: normal">
                    cancel</b> the booked appointment via a web site or a telephone call to a receptionist
                at DatDoc.<o:p></o:p></span></p>
        <div style="z-index: 101; left: 818px; width: 100px; position: absolute; top: 0px;
            height: 31px">
            <asp:HyperLink ID="HyperLink1" runat="server" NavigateUrl="~/HomePage.aspx" Width="186px">Login for Appointment</asp:HyperLink></div>
